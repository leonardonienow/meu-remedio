<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.meuRemedio.trabalho">

    <application
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteCRUDNoteAppKotlin"
        tools:ignore="AllowBackup">
        <activity
            android:name="br.meuRemedio.trabalho.activity.MainActivity"
            android:exported="true"
            android:label="Meus Remédios">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.meuRemedio.trabalho.activity.AddNoteActivity"
            android:exported="true"
            android:label="Adicionar Remédio"
            android:parentActivityName="br.meuRemedio.trabalho.activity.MainActivity" />
    </application>

</manifest>